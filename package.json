{
  "name": "dory-server",
  "version": "1.0.0",
  "description": "Dory Server back end",
  "main": "src/server.js",
  "scripts": {
    "run": "node src/server.js",
    "dev": "nodemon -w src src/app/server.js",
    "dev-install": "npm install -g nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "esExport": "npm run esExportUsers && npm run esExportSams",
    "esExportUsers": "npm run esExportUsersMapping && npm run esExportUsersData",
    "esExportUsersMapping": "./node_modules/.bin/elasticdump --input=http://localhost:9200/users --output=./data/users-mapping.json --type=mapping",
    "esExportUsersData": "./node_modules/.bin/elasticdump --input=http://localhost:9200/users --output=./data/users-data.json --type=data",
    "esExportSams": "npm run esExportSamsMapping && npm run esExportSamsData",
    "esExportSamsMapping": "./node_modules/.bin/elasticdump --input=http://localhost:9200/sam --output=./data/sams-mapping.json --type=mapping",
    "esExportSamsData": "./node_modules/.bin/elasticdump --input=http://localhost:9200/sam --output=./data/sams-data.json --type=data",
    "esImport": "npm run esImportUsers && npm run esImportSams",
    "esImportUsers": "npm run esImportUsersMapping && npm run esImportUsersData",
    "esImportUsersMapping": "./node_modules/.bin/elasticdump --output=http://localhost:9200/users --input=./data/users-mapping.json --type=mapping",
    "esImportUsersData": "./node_modules/.bin/elasticdump --output=http://localhost:9200/users --input=./data/users-data.json --type=data",
    "esImportSams": "npm run esImportSamsMapping && npm run esImportSamsData",
    "esImportSamsMapping": "./node_modules/.bin/elasticdump --output=http://localhost:9200/sam --input=./data/sams-mapping.json --type=mapping",
    "esImportSamsData": "./node_modules/.bin/elasticdump --output=http://localhost:9200/sam --input=./data/sams-data.json --type=data"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab-dei/dory/dory-server.git"
  },
  "keywords": [
    "Dory",
    "back",
    "end"
  ],
  "author": "Agrica",
  "license": "ISC",
  "dependencies": {
    "ajv": "^5.2.2",
    "ajv-async": "^0.1.0",
    "ajv-i18n": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "csv-parse": "^1.2.1",
    "elastic-builder": "^1.1.0",
    "elasticsearch": "^13.3.1",
    "http2": "^3.3.6",
    "jsonwebtoken": "^7.4.3",
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-jwt": "^3.2.2",
    "koa-passport": "^3.0.0",
    "koa-rbac": "^2.0.1",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "koa-unless": "^1.0.7",
    "morgan": "^1.8.2",
    "ms": "^2.0.0",
    "node-fetch": "^1.7.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "rotating-file-stream": "^1.3.0",
    "uuid": "^3.1.0",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "elasticdump": "^3.3.1",
    "nodemon": "^1.12.1"
  }
}
